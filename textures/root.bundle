hadow: #333 0 0 10px;\n    }\n/* End Help */\n\t\n/* Start controls */\n\t#controls {\n\t\tdisplay: none;\n\t\tposition: absolute;\n\t\theight: 30px;\n\t\ttop: 0;\n\t\tleft: 0;\n\t\tright: 0;\n\t\ttext-align: center;\n\t}\n\t#controls input[type=range] {\n\t\twidth: 15%;\n\t}\n\t\n\t#controls .btn {\n\t\tdisplay: inline-block;\n\t}\n\t\n/* Toggle Controlls */\n\t#toggleControls {\n\t\tdisplay: inline-block;\n\t\tmargin: 0px 7px;\n\t}\n\t\t\n\t#controls {\n\t\tmargin-left: 50px;\n\t}\n\t\n\tlabel {\n\t\tcursor: default !important;\n\t}\n\t\n/* http://www.cssportal.com/style-input-range/ */\n\tinput[type=range] {\n\t  cursor: ew-resize;\n\t  -webkit-appearance: none;\n\t  margin: 0;\n\t  width: 125px;\n\t}\n\tinput[type=range]:focus {\n\t  outline: none;\n\t}\n\tinput[type=range]::-webkit-slider-runnable-track {\n\t  width: 125px;\n\t  height: 5px;\n\t  cursor: pointer;\n/* \t  animate: 0.2s; */\n\t  background: #FFFFFF;\n\t  border-radius: 0px;\n\t  border: 1px solid #000;\n\t}\n\tinput[type=range]::-webkit-slider-thumb {\n\t  border: 1px solid #000;\n\t  height: 20px;\n\t  width: 20px;\n\t  border-radius: 25px;\n\t  background: #ECF0F1;\n\t  cursor: pointer;\n\t  -webkit-appearance: none;\n\t  margin-top: -7px;\n\t}\n\tinput[type=range]:focus::-webkit-slider-runnable-track {\n\t  background: #FFFFFF;\n\t}\n\tinput[type=range]::-moz-range-track {\n\t  width: 125px;\n\t  height: 5px;\n\t  cursor: pointer;\n/* \t  animate: 0.2s; */\n\t  background: #FFFFFF;\n\t  border-radius: 0px;\n\t  border: 1px solid #000;\n\t}\n\tinput[type=range]::-moz-range-thumb {\n\t  border: 1px solid #000;\n\t  height: 20px;\n\t  width: 20px;\n\t  border-radius: 25px;\n\t  background: #ECF0F1;\n\t  cursor: pointer;\n\t}\n\tinput[type=range]::-ms-track {\n\t  width: 125px;\n\t  height: 5px;\n\t  cursor: pointer;\n/* \t  animate: 0.2s; */\n\t  background: transparent;\n\t  border-color: transparent;\n\t  color: transparent;\n\t}\n\tinput[type=range]::-ms-fill-lower {\n\t  background: #888;\n\t  border: 1px solid #000;\n\t  border-radius: 0px;\n\t}\n\tinput[type=range]::-ms-fill-upper {\n\t  background: #FFFFFF;\n\t  border: 1px solid #000;\n\t  border-radius: 0px;\n\t}\n\tinput[type=range]::-ms-thumb {\n\t  border: 1px solid #000;\n\t  height: 20px;\n\t  width: 20px;\n\t  border-radius: 25px;\n\t  background: #ECF0F1;\n\t  cursor: pointer;\n\t}\n\tinput[type=range]:focus::-ms-fill-lower {\n\t  background: #888;\n\t}\n\tinput[type=range]:focus::-ms-fill-upper {\n\t  background: #FFFFFF;\n\t}\n/* End controls */\n\t\n/* Start Buttons */\n\t#buttons {\n\t\tdisplay: none;\n\t\tposition: absolute;\n\t\ttop: 35px;\n\t\tbottom: 40px;\n\t\tleft: 0;\n\t\t-webkit-touch-overflow: scroll;\n\t\t-webkit-overflow-scrolling: touch;\n\t}\n\t\n\t#buttons .btn {\n\t    display: block;\n\t}\n\n\t.btn, label {\n\t\tfont-size: 16px;\n\t\tcolor: #fff;\n\t\tbackground: transparent;\n\t\theight: 40px;\n\t\tborder: none;\n\t\tcursor: pointer;\n\t\t/* add border around text */\n\t\ttext-shadow:\n\t\t\t-1px -1px 0 #000,\n\t\t\t1px -1px 0 #000,\n\t\t\t-1px 1px 0 #000,\n\t\t\t1px 1px 0 #000;\n\t}\n\n/* \t.btn:active, */\n\t.btn:hover {\n\t\tcolor: #54A4FF\n\t}\n/* \t#panel {\n\t\tposition: absolute;\n\t\ttop: 35px;\n\t\tright: 0;\n\t\tbottom: 0;\n\t\twidth: 40%;\n\t\toverflow: auto;\n\t} */\n\t.info {\n\t\tdisplay: none;\n\t\tposition: fixed;\n\t\tpadding: 1px 1em;\n\t\tbackground: #fff;\n\t\ttop: 35px;\n\t\tright: 0;\n\t\tbottom: 0;\n\t\twidth: 40%;\n\t\toverflow: auto;\n\t\t-webkit-touch-overflow: scroll;\n\t\t-webkit-overflow-scrolling: touch;\n\t}\n\n\n\t/*  START TABS */\n\t/* https://css-tricks.com/functional-css-tabs-revisited/*/\n\t.tabs {\n\t\tposition: relative;\n \t\tmin-height: 600px; /* This part sucks */\n\t\tclear: both;\n\t\tmargin: 25px -16px;\n\t}\n\n\t.tab {\n\t\tfloat: left;\n\t}\n\n\t.tab label {\n\t\tcolor: #000; /* is a overwrite */\n\t\ttext-shadow: none; /* is a overwrite */\n\t\tbackground: #eee;\n\t\tpadding: 10px;\n\t\tborder: 1px solid #ccc;\n\t\tmargin-left: -1px;\n\t\tposition: relative;\n\t\tleft: 1px;\n\t}\n\n\t.tab .content {\n\t\tdisplay: none;\n\t\t\n\t\tposition: absolute;\n\t\ttop: 28px;\n\t\tleft: 0;\n\t\tbackground: #fff;\n\t\tright: 0;\n\t\tbottom: 0;\n\t\tpadding: 10px;\n\t\tborder-top: 1px solid #ccc;\n\t}\n\t\n\t.active .content {\n\t\tdisplay: block;\n\t}\n\t\n\t.active label {\n\t\tbackground: #fff;\n\t\tborder-bottom: 1px solid #fff;\n\t\tz-index: 2;\n\t}\n\t/*  END TABS */\n\n\t/*  START TABLE */\n\ttable {\n\t\tborder-collapse: collapse;\n\t\tborder-spacing: 0;\n\t\tdisplay: block;\n\t\tmargin-bottom: 16px;\n\t\t/* overflow: auto; */\n\t}\n\n\ttable tr {\n\t\tbackground: #fff;\n\t}\n\n\ttable tr:nth-child(2n) {\n\t\tbackground-color: #f1f1f1;\n\t}\n\n\ttable td,\n\ttable th {\n\t\tborder: 1px solid #ddd;\n\t\tpadding: 6px 13px;\n\t}\n/*  END TABLE */\n\t\n\t@media (max-height: 470px) {\n\t\t#infoPanel div {\n\t\t\tmargin 0;\n\t\t}\n\t\t#buttons {\n\t\t\tposition: fixed;\n\t\t\toverflow-y: scroll;\n\t\t}\n\t}\n</style>\n\n<!-- Start Help -->\n<div id=\"infoPanel\">\n\t<div>\n\t\t<span class=\"fa fa-times fa-2x\" title=\"Close\"></span>\n\t\t<h3>Mouse</h3>\n\t\t<p>Rotate: click &amp; hold left mouse button and move mouse.</p>\n\t\t<p>Zoom: scroll wheel (up/down).</p>\n\t\t<p>Move: click &amp; hold right mouse button and move mouse.</p>\n\t\t<h3>Touch</h3>\n\t\t<p>Rotate: tap and drag your finger.</p>\n\t\t<p>Zoom: pinch with two fingers.</p>\n\t\t<p>Move: tap and drag with two fingers.</p>\n\t\t<br>\n\t\t<small>To show advanced controls hit the 'O' key on your keyboard.</small>\n\t</div>\n</div>\n<!-- End Help -->\n\n<div id=\"toggleControls\" class=\"noselect btn\" >\n<!-- \tstyle=\"display:none;\" -->\n\t<span class=\"fa fa-sliders fa-2x\" title=\"show/hide buttons\"></span>\n</div>\n\n<!-- Start controls -->\n<div id=\"controls\" class=\"noselect\">\n\t<div id=\"container\">\n\t\t<button class=\"btn\" id=\"default-btn\">Default</button>\n\t\t<button class=\"btn\" id=\"distance-btn\">Distance</button>\n\t\t<button class=\"btn\" id=\"size-btn\">Size</button>\n\t\t<label for=\"speed\">Speed:</label>\n\t\t<input type=\"range\" id=\"speed\" min=\"1\" value=\"50000\" max=\"5000000\" step=\"10\">\n\t\t<button class=\"btn\" id=\"music-btn\"><span class=\"fa fa-volume-off fa-2x fa-fw\" title=\"Toggle Music\"></span></button>\n\t\t<button class=\"btn\" id=\"help-btn\"><span class=\"fa fa-question fa-2x\" title=\"Help\"></span></button>\n\t\t<button class=\"btn\" id=\"full-screen-btn\"><span class=\"fa fa-expand fa-2x\" title=\"Full-Screen\"></span></button>\n\t\t<button class=\"btn\" id=\"fps-btn\">Show FPS</button>\n\t</div>\n</div>\n<!-- End controls -->\n<!-- Start SIDEBAR -->\n<div id=\"buttons\" class=\"noselect\">\n    <button class=\"btn\" id=\"sun-btn\">Sun</button>\n    <button class=\"btn\" id=\"mercury-btn\">Mercury</button>\n    <button class=\"btn\" id=\"venus-btn\">Venus</button>\n    <button class=\"btn\" id=\"earth-btn\">Earth</button>\n    <button class=\"btn\" id=\"mars-btn\">Mars</button>\n    <button class=\"btn\" id=\"jupiter-btn\">Jupiter</button>\n    <button class=\"btn\" id=\"saturn-btn\">Saturn</button>\n    <button class=\"btn\" id=\"uranus-btn\">Uranus</button>\n    <button class=\"btn\" id=\"neptune-btn\">Neptune</button>\n    <button class=\"btn\" id=\"credits-btn\">Credits</button>\n</div>\n<div id=\"panel\">\n    <div class=\"info\" id=\"sun-info\">\n        <h2>Sun</h2>\n        <div class=\"tabs\">\n            <div class=\"tab active\">\n                <label>Description</label>\n                <div class=\"content\">\n                    <p>The Sun is the center star of our Solar System. All of the planets, asteroids and comets revolve around the Sun. The Sun\u2019s role as the center of the system comes from its high mass. It has 99.8% of the mass in the Solar System. The sun is a star, a hot ball of glowing gases. Without the sun's intense energy and heat, there would be no life on Earth. And though it is special to us, there are billions of stars like our sun scattered across the Milky Way galaxy.</p>\n                </div>\n            </div>\n            <div class=\"tab\">\n                <label>Facts</label>\n                <div class=\"content\">\n                    <table>\n                        <tbody>\n                            <tr>\n                                <td>Type</td>\n                                <td>Star</td>\n                                <td>G2V</td>\n                            </tr>\n                            <tr>\n                                <td>Luminosity</td>\n                                <td>3.83 x 10<sup>33</sup> ergs/sec</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Visual Brightness (V)</td>\n                                <td>-26.74</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Absolute Magnitude</td>\n                                <td>4.83</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Composition</td>\n                                <td>92.1% Hydrogen, 7.8% Helium</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Temperature</td>\n                                <td>5,504 \u00b0C</td>\n                                <td>5777 K</td>\n                            </tr>\n                            <tr>\n                                <td>Mass</td>\n                                <td>1.989 x 10<sup>30</sup> kg</td>\n                                <td>333,060.402 x Earth</td>\n                            </tr>\n                            <tr>\n                                <td>Equatorial Radius</td>\n                                <td>695,508 km</td>\n                                <td>109.2 x Earth</td>\n                            </tr>\n                            <tr>\n                                <td>Gravity</td>\n                                <td>274.0 m/s<sup>2</sup></td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Rotation Period</td>\n                                <td>25.38 Earth Days</td>\n                                <td>609.12 Hours</td>\n                            </tr>\n                            <tr>\n                                <td>Age</td>\n                                <td>4.6 Billion Years</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"info\" id=\"mercury-info\">\n        <h2>Mercury</h2>\n        <div class=\"tabs\">\n            <div class=\"tab active\">\n                <label>Description</label>\n                <div class=\"content\">\n                    <p>Mercury is the closest planet to the Sun and smallest of the terrestrial planets. It has a very thin atmosphere, which is only a little more substantial than a vacuum.</p>\n                </div>\n            </div>\n            <div class=\"tab\">\n                <label>Facts</label>\n                <div class=\"content\">\n                    <table>\n                        <tbody>\n                            <tr>\n                                <td>Type</td>\n                                <td>Terrestrial</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mass</td>\n                                <td>3.3010 x 10<sup>23</sup> kg</td>\n                                <td>0.055 x Earth</td>\n                            </tr>\n                            <tr>\n                                <td>Diamiter</td>\n                                <td>4,879 km</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Gravity</td>\n                                <td>3.7 m/s<sup>2</sup></td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Surface Temperature</td>\n                                <td>-173 to + 427 \u00b0C</td>\n                                <td>100 to 700 K</td>\n                            </tr>\n                            <tr>\n                                <td>Rotation Period</td>\n                                <td>58.646 Earth Days</td>\n                                <td>1407.5 Hours</td>\n                            </tr>\n                            <tr>\n                                <td>Atmosphere</td>\n                                <td>vacuum</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Ring System?</td>\n                                <td>No</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Global Magnetic Field?</td>\n                                <td>Yes</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>No. of Moons</td>\n                                <td>0</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Distance from Sun</td>\n                                <td>57,909,227 km</td>\n                                <td>0.38709927 AU</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Orbit Velocity</td>\n                                <td>170,503 km/h</td>\n                                <td>4.7362 x 10<sup>4</sup> m/s</td>\n                            </tr>\n                            <tr>\n                                <td>Orbit Period</td>\n                                <td>88 Earth Days</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"info\" id=\"venus-info\">\n        <h2>Venus</h2>\n        <div class=\"tabs\">\n            <div class=\"tab active\">\n                <label>Description</label>\n                <div class=\"content\">\n                    <p>Venus has a very thick atmosphere mostly composed of carbon dioxide gas. The thick carbon dioxide atmosphere traps heat from the Sun during the day and does not let the surface cool at night. The high temperature and unbreathable thick atmosphere would make the planet very inhospitable to human visitors.</p>\n                </div>\n            </div>\n            <div class=\"tab\">\n                <label>Facts</label>\n                <div class=\"content\">\n                    <table>\n                        <tbody>\n                            <tr>\n                                <td>Type</td>\n                                <td>Terrestrial</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mass</td>\n                                <td>4.8673 x 10<sup>24</sup> kg</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Diamiter</td>\n                                <td>12,104 km</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Gravity</td>\n                                <td>8.87 m/s<sup>2</sup></td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Surface Temperature</td>\n                                <td>462 \u00b0C</td>\n                                <td>735 K</td>\n                            </tr>\n                            <tr>\n                                <td>Rotation Period</td>\n                                <td>-243.018 Earth Days</td>\n                                <td>-5832.4 Hours</td>\n                            </tr>\n                            <tr>\n                                <td>Atmosphere</td>\n                                <td>Carbon Dioxide, Nitrogen</td>\n                                <td>CO<sub>2</sub>, N<sub>2</sub></td>\n                            </tr>\n                            <tr>\n                                <td>Ring System?</td>\n                                <td>No</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Global Magnetic Field?</td>\n                                <td>No</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>No. of Moons</td>\n                                <td>0</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Distance from Sun</td>\n                                <td>108,209,475 km</td>\n                                <td>0.72333566 AU</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Orbit Velocity</td>\n                                <td>126,074 km/h</td>\n                                <td>3.5020 x 10<sup>4</sup> m/s</td>\n                            </tr>\n                            <tr>\n                                <td>Orbit Period</td>\n                                <td>224.7 Earth Days</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"info\" id=\"earth-info\">\n        <h2>Earth</h2>\n        <div class=\"tabs\">\n            <div class=\"tab active\">\n                <label>Description</label>\n                <div class=\"content\">\n                    <p>Earth is our home planet. Most of its surface (over 70%) is covered with oceans, with the remainder featuring a wide variety of land forms, from mountains and valleys to plains and beaches. Earth has a thick atmosphere, which is mostly nitrogen (78%) and oxygen (21%). The region on and below the surface of Earth is filled with life which makes this planet unique.</p>\n                </div>\n            </div>\n            <div class=\"tab\">\n                <label>Facts</label>\n                <div class=\"content\">\n                    <table>\n                        <tbody>\n                            <tr>\n                                <td>Type</td>\n                                <td>Terrestrial</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mass</td>\n                                <td>5.9722 x 10<sup>24</sup> kg</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Diamiter</td>\n                                <td>12,756 km</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Gravity</td>\n                                <td>9.80665 m/s<sup>2</sup></td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Surface Temperature</td>\n                                <td>-88 to + 58 \u00b0C</td>\n                                <td>185 to 331 K</td>\n                            </tr>\n                            <tr>\n                                <td>Rotation Period</td>\n                                <td>0.99726968 Earth Days</td>\n                                <td>23.934 Hours</td>\n                            </tr>\n                            <tr>\n                                <td>Atmosphere</td>\n                                <td>Nitrogen, Oxygen</td>\n                                <td>N<sub>2</sub>, O<sub>2</sub></td>\n                            </tr>\n                            <tr>\n                                <td>Ring System?</td>\n                                <td>No</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Global Magnetic Field?</td>\n                                <td>Yes</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>No. of Moons</td>\n                                <td>1</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Distance from Sun</td>\n                                <td>149,598,262 km</td>\n                                <td>1 AU</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Orbit Velocity</td>\n                                <td>107,218 km/h</td>\n                                <td>2.9783 x 10<sup>4</sup> m/s</td>\n                            </tr>\n                            <tr>\n                                <td>Orbit Period</td>\n                                <td>1 Earth Years</td>\n                                <td>365.2 Earth Days</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n        <h2>Moon</h2>\n        <div class=\"tabs\">\n            <div class=\"tab active\">\n                <label>Description</label>\n                <div class=\"content\">\n                    <p>The moon revolves around Earth and determines Earth\u2019s tides. The Moon is our natural satellite. So far is the only object in space humans have travelled to. Neil Armstrong is the first human to step on the moon on the 20th of July 1969.</p>\n                </div>\n            </div>\n            <div class=\"tab\">\n                <label>Facts</label>\n                <div class=\"content\">\n                    <table>\n                        <tbody>\n                            <tr>\n                                <td>Mass</td>\n                                <td>7.3477 x 10<sup>22</sup> kg</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Diamiter</td>\n                                <td>3,475 km</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Gravity</td>\n                                <td>1.624 m/s<sup>2</sup></td>\n                                <td>0.166 x Earth</td>\n                            </tr>\n                            <tr>\n                                <td>Surface Temperature</td>\n                                <td>-233 to +123 \u00b0C</td>\n                                <td>40 to 396 K</td>\n                            </tr>\n                            <tr>\n                                <td>Rotation Period</td>\n                                <td>27.322 Earth Days</td>\n                                <td>655.73 Hours</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Distance from Earth</td>\n                                <td>384,400 km</td>\n                                <td>0.00257 AU</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Orbit Velocity</td>\n                                <td>3,680.5 km/h</td>\n                                <td>1,022 m/s</td>\n                            </tr>\n                            <tr>\n                                <td>Orbit Period</td>\n                                <td>0.074803559 Earth Years</td>\n                                <td>27.322 Earth Days</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"info\" id=\"mars-info\">\n        <h2>Mars</h2>\n        <div class=\"tabs\">\n            <div class=\"tab active\">\n                <label>Description</label>\n                <div class=\"content\">\n                    <p>Mars is about half the size of Earth in diameter. Mars has a carbon dioxide atmosphere, but it is extremely thin. The thin air does not retain heat well. Mars has polar ice caps, made of water ice and carbon dioxide ice. The Martian surface has features that look like dry stream-beds, leading many researchers to suspect that at some time in the distant past, Mars may have had liquid water flowing on its surface.</p>\n                </div>\n            </div>\n            <div class=\"tab\">\n                <label>Facts</label>\n                <div class=\"content\">\n                    <table>\n                        <tbody>\n                            <tr>\n                                <td>Type</td>\n                                <td>Terrestrial</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mass</td>\n                                <td>6.4169 x 10<sup>23</sup> kg</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Diamiter</td>\n                                <td>6,792 km</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Gravity</td>\n                                <td>3.71 m/s<sup>2</sup></td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Surface Temperature</td>\n                                <td>-153 to +20 \u00b0C</td>\n                                <td>120 to 293 K</td>\n                            </tr>\n                            <tr>\n                                <td>Rotation Period</td>\n                                <td>1.026 Earth Days</td>\n                                <td>24.623 Hours</td>\n                            </tr>\n                            <tr>\n                                <td>Atmosphere</td>\n                                <td>Carbon Dioxide, Nitrogen, Argon</td>\n                                <td>CO<sub>2</sub>, N<sub>2</sub>, Ar</td>\n                            </tr>\n                            <tr>\n                                <td>Ring System?</td>\n                                <td>No</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Global Magnetic Field?</td>\n                                <td>No</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>No. of Moons</td>\n                                <td>2</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Distance from Sun</td>\n                                <td>227,943,824 km</td>\n                                <td>1.523662 AU</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Orbit Velocity</td>\n                                <td>86,677 km/h</td>\n                                <td>2.4077 x 10<sup>4</sup> m/s</td>\n                            </tr>\n                            <tr>\n                                <td>Orbit Period</td>\n                                <td>687 Earth Days</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"info\" id=\"jupiter-info\">\n        <h2>Jupiter</h2>\n        <div class=\"tabs\">\n            <div class=\"tab active\">\n                <label>Description</label>\n                <div class=\"content\">\n                    <p>Jupiter is the largest planet in the Solar System. It is about 318 times as massive as Earth, and over 1,300 Earths could fit inside of it. Like all Jovian planets, Jupiter\u2019s surface shows complicated wind patterns. Perhaps the most recognizable feature on Jupiter\u2019s surface is the Great Red Spot, a huge storm, twice the diameter of Earth, which has been going on for at least 300 years.</p>\n                </div>\n            </div>\n            <div class=\"tab\">\n                <label>Facts</label>\n                <div class=\"content\">\n                    <table>\n                        <tbody>\n                            <tr>\n                                <td>Type</td>\n                                <td>Jovian</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mass</td>\n                                <td>1.8981 x 10<sup>27</sup> kg</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Diamiter</td>\n                                <td>142,984 km</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Gravity</td>\n                                <td>24.79 m/s<sup>2</sup></td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Temperature</td>\n                                <td>-148 \u00b0C</td>\n                                <td>125 K</td>\n                            </tr>\n                            <tr>\n                                <td>Rotation Period</td>\n                                <td>0.41354 Earth Days</td>\n                                <td>9.92496 Hours</td>\n                            </tr>\n                            <tr>\n                                <td>Atmosphere</td>\n                                <td>Hydrogen, Helium</td>\n                                <td>H<sub>2</sub>, He</td>\n                            </tr>\n                            <tr>\n                                <td>Ring System?</td>\n                                <td>Yes</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Global Magnetic Field?</td>\n                                <td>Yes</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>No. of Moons</td>\n                                <td>67</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Distance from Sun</td>\n                                <td>778,340,821 km</td>\n                                <td>5.2028870 AU</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Orbit Velocity</td>\n                                <td>47,002 km/h</td>\n                                <td>1.3056 x 10<sup>4</sup> m/s</td>\n                            </tr>\n                            <tr>\n                                <td>Orbit Period</td>\n                                <td>4,331 Earth Days</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"info\" id=\"saturn-info\">\n        <h2>Saturn</h2>\n        <div class=\"tabs\">\n            <div class=\"tab active\">\n                <label>Description</label>\n                <div class=\"content\">\n                    <p>Saturn is just a little smaller than Jupiter (its diameter is about 85% of Jupiter\u2019s) but a lot lighter (its mass is about a third of Jupiter\u2019s). This means that it has a very low density. In fact, its density is the lowest of all the planets and less than the density of water. In composition and internal structure, the planet is thought to be fairly similar to its larger sibling, Jupiter.</p>\n                    <p>All Jovian planets are surrounded by a complex ring system made of icy particles. Saturn\u2019s ring system is, by far, the most spectacular. The rings of the other Jovian planets are much thinner and fainter.</p>\n                </div>\n            </div>\n            <div class=\"tab\">\n                <label>Facts</label>\n                <div class=\"content\">\n                    <table>\n                        <tbody>\n                            <tr>\n                                <td>Type</td>\n                                <td>Jovian</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mass</td>\n                                <td>5.6832 x 10<sup>26</sup> kg</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Diamiter</td>\n                                <td>120,536 km</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Gravity</td>\n                                <td><sup>*</sup>10.4 m/s<sup>2</sup></td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Temperature</td>\n                                <td>-178 \u00b0C</td>\n                                <td>95 K</td>\n                            </tr>\n                            <tr>\n                                <td>Rotation Period</td>\n                                <td>0.444 Earth Days</td>\n                                <td>10.656 Hours</td>\n                            </tr>\n                            <tr>\n                                <td>Atmosphere</td>\n                                <td>Hydrogen, Helium</td>\n                                <td>H<sub>2</sub>, He</td>\n                            </tr>\n                            <tr>\n                                <td>Ring System?</td>\n                                <td>Yes</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Global Magnetic Field?</td>\n                                <td>Yes</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>No. of Moons</td>\n                                <td>62</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Distance from Sun</td>\n                                <td>1,426,666,422 km</td>\n                                <td>9.53667594 AU</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Orbit Velocity</td>\n                                <td>34,701 km/h</td>\n                                <td>9.6391 x 10<sup>3</sup> m/s</td>\n                            </tr>\n                            <tr>\n                                <td>Orbit Period</td>\n                                <td>10,747 Earth Days</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"info\" id=\"uranus-info\">\n        <h2>Uranus</h2>\n        <div class=\"tabs\">\n            <div class=\"tab active\">\n                <label>Description</label>\n                <div class=\"content\">\n                    <p>Uranus\u2019s unique feature is that it appears to have been knocked over sometime in the past. Spinning perpendicular to it's orbit. Most planets orbit around the Sun spinning upright. The cause of this is not yet certain, but it has been suggested that it was caused by an impact of a large object, such as a large asteroid or moon. Giant impacts like this were common during the early history of the Solar System; a similar impact is thought to have created the Earth\u2019s Moon.</p>\n                </div>\n            </div>\n            <div class=\"tab\">\n                <label>Facts</label>\n                <div class=\"content\">\n                    <table>\n                        <tbody>\n                            <tr>\n                                <td>Type</td>\n                                <td>Jovian</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mass</td>\n                                <td>8.6810 x 10<sup>25</sup> kg</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Diamiter</td>\n                                <td>51,118 km</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Gravity</td>\n                                <td>8.87 m/s<sup>2</sup></td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Temperature</td>\n                                <td>-216 \u00b0C</td>\n                                <td>57 K</td>\n                            </tr>\n                            <tr>\n                                <td>Rotation Period</td>\n                                <td>-0.718 Earth Days</td>\n                                <td>-17.23992 Hours</td>\n                            </tr>\n                            <tr>\n                                <td>Atmosphere</td>\n                                <td>Hydrogen, Helium, Methane</td>\n                                <td>H<sub>2</sub>, He, CH<sub>4</sub></td>\n                            </tr>\n                            <tr>\n                                <td>Ring System?</td>\n                                <td>Yes</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Global Magnetic Field?</td>\n                                <td>Yes</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>No. of Moons</td>\n                                <td>27</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Distance from Sun</td>\n                                <td>2,870,658,186 km</td>\n                                <td>19.189165 AU</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Orbit Velocity</td>\n                                <td>24,477 km/h</td>\n                                <td>6.7991 x 10<sup>3</sup> m/s</td>\n                            </tr>\n                            <tr>\n                                <td>Orbit Period</td>\n                                <td>30,589 Earth Days</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"info\" id=\"neptune-info\">\n        <h2>Neptune</h2>\n        <div class=\"tabs\">\n            <div class=\"tab active\">\n                <label>Description</label>\n                <div class=\"content\">\n                    <p>Neptune is similar in size to Uranus. Giant storm centers can be seen on its visible surface, similar to those on the other Jovian planets. The atmosphere features great wind patterns; its winds are the fastest in the Solar System, reaching speeds of 2,000 km/hour (or 1,200 miles/hour).</p>\n                </div>\n            </div>\n            <div class=\"tab\">\n                <label>Facts</label>\n                <div class=\"content\">\n                    <table>\n                        <tbody>\n                            <tr>\n                                <td>Type</td>\n                                <td>Jovian</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mass</td>\n                                <td>1.0241 x 10<sup>26</sup> kg</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Diamiter</td>\n                                <td>49,528 km</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Gravity</td>\n                                <td>11.15 m/s<sup>2</sup></td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Temperature</td>\n                                <td>-214 \u00b0C</td>\n                                <td>59 K</td>\n                            </tr>\n                            <tr>\n                                <td>Rotation Period</td>\n                                <td>0.671 Earth Days</td>\n                                <td>16.11000 Hours</td>\n                            </tr>\n                            <tr>\n                                <td>Atmosphere</td>\n                                <td>Hydrogen, Helium, Methane</td>\n                                <td>H<sub>2</sub>, He, CH<sub>4</sub></td>\n                            </tr>\n                            <tr>\n                                <td>Ring System?</td>\n                                <td>Yes</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Global Magnetic Field?</td>\n                                <td>Yes</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>No. of Moons</td>\n                                <td>14</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Distance from Sun</td>\n                                <td>4,498,396,441 km</td>\n                                <td>30.069923 AU</td>\n                            </tr>\n                            <tr>\n                                <td>Mean Orbit Velocity</td>\n                                <td>19,566 km/h</td>\n                                <td>5.4349 x 10<sup>3</sup> m/s</td>\n                            </tr>\n                            <tr>\n                                <td>Orbit Period</td>\n                                <td>59,800 Earth Days</td>\n                                <td>&nbsp;</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"info\" id=\"credits-info\">\n        <h2>Credits</h2>\n        <p>Developed by: <a href=\"https://github.com/publicarray\">Sebastian Schmidt</a></p>\n        <p>Lesson Information and Text: <a href=\"http://www.messenger-education.org/teachers/Modules/Lessons/ourss.pdf\">messenger-education.org</a></p>\n        <p>Textures: <a href=\"http://planetpixelemporium.com/planets.html\">planetpixelemporium.com</a>, \n\t\t<a href=\"http://visibleearth.nasa.gov/view_cat.php?categoryID=1484\">visibleearth.nasa.gov</a> and <a href=\"http://sourceforge.net/p/celestia/code/HEAD/tree/trunk/celestia/\">Celestia</a></p>\n\t\t<p>Normal Map Generation: <a href=\"https://cpetry.github.io/NormalMap-Online/\">cpetry.github.io/NormalMap-Online</a></p>\n        <p>Skybox: <a href=\"http://paulbourke.net/miscellaneous/starfield\">paulbourke.net</a></p>\n        <p>Music: <a href=\"http://www.jamendo.com/en/artist/358490/stellardrone\">Stellardrone</a></p>\n        <p>Data: <a href=\"http://nssdc.gsfc.nasa.gov/planetary/factsheet\">NASA</a></p>\n    </div>\n</div>\n<!-- END SIDEBAR -->\n"}, "transform": {"rotation": [0, 0, 0], "scale": [1, 1, 1], "translation": [0, 0, 0]}, "script": {"scripts": {"204af25e0ab547a7bb5469ba77916828.scriptInstance": {"id": "204af25e0ab547a7bb5469ba77916828.scriptInstance", "name": "Custom script", "scriptRef": "48702da0a4374057ab36cffda07931b6.script", "sortValue": 1, "options": {"enabled": true}}}}}, "hidden": false, "id": "e6a2d897e2e345bf9ceaa5cccab4f381.entity"}, "c7357214b5b74502abde826840d155a3.texture": {"wrapS": "Repeat", "minFilter": "Trilinear", "imageRef": "5ae5840c895528b58f6d84f609f152c55eae5d26.jpg", "license": "CC0", "flipY": false, "deleted": false, "name": "venusmap.jpg", "magFilter": "Bilinear", "created": "2015-09-03T10:02:16.587Z", "dataModelVersion": 2, "modified": "2015-09-03T10:02:16.587Z", "matchFileName": "venusmap.jpg", "repeat": [1, 1], "lodBias": 0, "offset": [0, 0], "id": "c7357214b5b74502abde826840d155a3.texture", "generateMipmaps": true, "anisotropy": 1, "wrapT": "Repeat"}, "89d0242959804482babbfc1f9